<div class="listing-card listing-card-block listing-card-connector search-item result-item view-item component-obj-view component-listing-view"
     i18n:domain="novaideo"
     id="listingbloc_${oid}"
     data-component_type="object-view"
     i18n:domain="novaideo"
     tal:define="icon getattr(object, 'icon');
                 oid object.__oid__;
                 ">
  <div class="listing-card-img" 
       style="
              background-color: #db4437;">
    <div class="object-menu pull-right">
      <div tal:replace="structure menu_body"/>
    </div>
    <div class="img-container">
      <img class="img-circle"
          tal:attributes="src request.static_url('novaideo:static/connectors/google/images/google.png')"  width="70"/>
    </div>
  </div>
  <div class="listing-card-container">
    <div class="listing-card-content">
      <div>
        <div class="listing-card-data">
            <span class="listing-card-title">
                  <span i18n:translate="">Google connector</span>
            </span>
          <div class="listing-card-addon">
            <small>
              <div i18n:translate="">The Google connector</div>
            </small>
          </div>
        </div>
      </div>
      <div class="listing-card-footer">
        <div class="actions-block">
          <div tal:replace="structure footer_body"/>
        </div>
      </div>
    </div>
  </div>
    <div class="action-interation-container action-slider-container"
         data-interaction_kind="slider">
      <button class="btn btn-primary action-slider-btn"><span class="glyphicon glyphicon-chevron-right"></span></button>
      <div class="container-body">
      </div>
  </div>
</div>